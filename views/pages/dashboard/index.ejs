            <div class="col-lg-12">
                <div class="card text-white bg-brown dashboard-box">
                    <div class="card-body">
                        <h3 class="mb-0 fw-r" style="text-align: center;"> Denah Kamar </h3>
                    </div>
                </div>
            </div>

          <div class="col-sm-12 col-md-12 col-lg-12">
                <select name="id" id="id" class="form-control">
                    <option value="">Pilih Lantai</option>
                    <% lantai.forEach(row => { %>
                        <option value="<%= row.id %>"><%= row.lantai %></option>
                    <% }) %>
                </select>
              <br>
            <% if (result == 0 && dataAsramaL == null) { %> 
            <table class="table table-striped" id="tableAsramaL" style="border-color: black;">
                <tr>
                    <td class="text-center font-weight-bold">Tidak ada data</td>
                </tr>
            </table>
            <% } else { %>
                <table class="table table-hover table-striped table-bordered tableAsrama" id="tableL">
                    <tr>
                        <th class="" colspan="8" style="text-align: center;">
                            <% if (result.length > 0) { %> 
                                <span class="badge badge-pill badge-success" style='font-weight: normal;'><i class='fa fa-university'></i>
                                    <b><%= typeof result[0].nama_asrama != 'undefined' ? result[0].nama_asrama : '' %></b>
                                </span>
                                <br>
                                <small><b>Lantai : <%= typeof result[0].id_lantai != 'undefined' ? result[0].id_lantai : '' %></b></small>
                            <% } else { %>
                                <% if (dataAsramaL.length > 0) { %>
                                    <span class="badge badge-pill badge-success" style='font-weight: normal;'><i class='fa fa-university'></i>
                                        <b><%= typeof dataAsramaL[0].nama_asrama != 'undefined' ? dataAsramaL[0].nama_asrama : '' %></b>
                                    </span>
                                    <br>
                                    <small><b>Lantai : <%= typeof dataAsramaL[0].lantai != 'undefined' ? dataAsramaL[0].lantai : '' %></b></small>
                                <% } %>
                            <% } %>
                        </th>
                    </tr>
                        <% if (result.length > 0) { %>
                            <% var col = 6 %>
                            <% var row = Math.ceil(result.length / col) %>
                            <% var count = 0 %>
                            <% for (var i = 1; i <= row; i++) { %>  
                                <tr>
                                <% for (var j = 1; j <= col; j++) { %>
                                    <% if (result.nama_asrama = 'A (Laki-laki)') { %>
                                        <% if (result[count] != undefined && result[count] != null) { %>
                                            <td <% if (result[count].status == 0) { %> class="text-white bg-success text-center" data-toggle='tooltip' data-placement='top' title='Kamar tersedia' <% } else { %> class="text-white text-center bg-danger" data-toggle='tooltip' data-placement='top' title='Kamar tidak tersedia' <% } %>>
                                                <% if (session.data.kewenangan_id == 1) { %>
                                                    <a href="/kamar/detail/<%= result[count].id %>" class="text-white"><%= result[count].nama_kamar %></a>
                                                <% } else { %>
                                                    <a href="#" class="text-white"><%= result[count].nama_kamar %></a>
                                                <% } %>  
                                            </td>
                                            <% count++ %>
                                        <% } else { %>
                                            <% continue %>
                                        <% } %>
                                        <% count++ %>
                                    <% } %>
                                <% } %>
                                </tr>
                                <% if (i < row) { %>
                                    <tr>
                                        <td colspan="8" style="text-align: center;"></td>
                                    </tr>
                                    <% continue %>
                                <% } %>
                            <% } %>
                        <% } else { %> 
                            <% var col = 6 %>
                            <% var row = Math.ceil(dataAsramaL.length / col) %>
                            <% var count = 0 %>
                            <% for (var i = 1; i <= row; i++) { %>  
                                <tr>
                                <% for (var j = 1; j <= col; j++) { %>
                                    <% if (dataAsramaL[count] != undefined && dataAsramaL[count] != null) { %>
                                        <td <% if (dataAsramaL[count].status == 0) { %> class="text-white text-center bg-success" data-toggle='tooltip' data-placement='top' title='Kamar tersedia' <% } else { %> class="text-white text-center bg-danger" data-toggle='tooltip' data-placement='top' title='Kamar tidak tersedia' <% } %>>
                                            <% if (session.data.kewenangan_id == 1) { %>
                                                <a href="/kamar/detail/<%= dataAsramaL[count].id %>" class="text-white"><%= dataAsramaL[count].nama_kamar %></a>
                                            <% } else { %>
                                                <a href="#" class="text-white"><%= dataAsramaL[count].nama_kamar %></a>
                                            <% } %>  
                                        </td>
                                        <% count++ %>
                                    <% } else { %>
                                        <% continue %>
                                    <% } %>
                                <% } %>
                                </tr>
                                <% if (i < row) { %>
                                    <tr>
                                        <td colspan="8" style="text-align: center;"></td>
                                    </tr>
                                <% } %>
                            <% } %>
                        <% } %>
                </table>
            <% } %>
        <br>
        <% if (result.length == 0 && dataAsramaP == null) { %> 
            <table class="table table-striped table-bordered" id="tableAsramaP">
                <tr>
                    <td class="text-center font-weight-bold">Tidak ada data</td>
                </tr>
            </table>
            <% } else { %>
            <table class="table table-striped table-bordered tableAsrama" id="tableAsramaP">
                <tr>
                    <th class="" colspan="8" style="text-align: center;">
                        <% if (result.length > 0) { %> 
                                <% for (let i of result) { %> 
                                    <% if (i.nama_asrama == 'B (Perempuan)') { %>
                                        <span class="badge badge-pill badge-success" style='font-weight: normal;'><i class='fa fa-university'></i>
                                            <b><%= typeof i.nama_asrama != 'undefined' ? i.nama_asrama : '' %></b>
                                        </span>
                                        <% break %>
                                    <% } %>
                                <% } %>
                                    <br>
                                    <small><b>Lantai : <%= typeof result[0].id_lantai != 'undefined' ? result[0].id_lantai : '' %></b></small>
                        <% } else { %>
                                <% if (dataAsramaP.length > 0) { %> 
                                    <span class="badge badge-pill badge-success" style='font-weight: normal;'><i class='fa fa-university'></i>
                                        <b><%= typeof dataAsramaP[0].nama_asrama != 'undefined' ? dataAsramaP[0].nama_asrama : '' %></b>
                                    </span>
                                    <br>
                                    <small><b>Lantai : <%= typeof dataAsramaP[0].lantai != 'undefined' ? dataAsramaP[0].lantai : '' %></b></small>
                                <% } %>
                        <% } %>
                    </th>
                </tr>
                <!-- <tr> -->
                    <% if (result.length > 0) { %>
                        <% var col = 6 %>
                        <% var count = 0 %>
                        <% var row = Math.ceil(result.length / col) %>
                        <% for (var i = 1; i<= row; i++) { %> 
                            <tr>
                            <% for (var j = 1; j<= col; j++) { %>
                                <% if (result.nama_asrama = 'B (Perempuan)') { %>
                                    <% count++ %>
                                    <% if (result[count] !== undefined && result[count] !== null) { %>
                                        <td <% if (result[count].status == 0) { %> class="text-white text-center bg-success" data-toggle='tooltip' data-placement='top' title='Kamar tersedia' <% } else { %> class="text-white text-center bg-danger" data-toggle='tooltip' data-placement='top' title='Kamar tidak tersedia' <% } %>>
                                            <% if (session.data.kewenangan_id == 1) { %>
                                                <a href="/kamar/detail/<%= result[count].id %>" class="text-white"><%= result[count].nama_kamar %></a>
                                            <% } else { %>
                                                <a href="#" class="text-white"><%= result[count].nama_kamar %></a>
                                            <% } %>
                                        </td>   
                                        <% count++ %>
                                    <% } else { %>
                                        <% continue %>
                                    <% } %>   
                                <% } %>
                            <% } %>
                            </tr>
                            <% if (i < row) { %>
                                <tr>
                                    <td class="break" colspan="8" style="text-align: center;"></td>
                                </tr>
                                
                            <% } %>
                        <% } %>
                    <% } else { %> 
                            <% var col = 6 %>
                            <% var count = 0 %>
                            <% var row = Math.ceil(dataAsramaP.length / col) %>
                            <% for (var i = 1; i<= row; i++) { %> 
                                <tr>
                                <% for (var j = 1; j<= col; j++) { %>
                                    <% if (dataAsramaP[count] !== undefined && dataAsramaP[count] !== null) { %>
                                        <td <% if (dataAsramaP[count].status == 0) { %> class="text-white text-center bg-success" data-toggle='tooltip' data-placement='top' title='Kamar tersedia' <% } else { %> class="text-white text-center bg-danger" data-toggle='tooltip' data-placement='top' title='Kamar tidak tersedia' <% } %>>
                                            <% if (session.data.kewenangan_id == 1) { %>
                                                <a href="/kamar/detail/<%= dataAsramaP[count].id %>" class="text-white"><%= dataAsramaP[count].nama_kamar %></a>
                                            <% } else { %>
                                                <a href="#" class="text-white"><%= dataAsramaP[count].nama_kamar %></a>
                                            <% } %>
                                        </td>   
                                        <% count++ %>
                                    <% } else { %>
                                        <% continue %>
                                    <% } %>
                                <% } %>
                                </tr>
                                <% if (i < row) { %>
                                    <tr>
                                        <td class="break" colspan="8" style="text-align: center;"></td>
                                    </tr>
                                <% } %>
                            <% } %>
                    <% } %>
            </table>
        <% } %>
          </div>
    </div>
</div>

