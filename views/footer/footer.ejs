            $(".toggle-password").click(function() {
                $(this).toggleClass("fa-eye fa-eye-slash");
                    var input = $($(this).attr("toggle"));
                    if (input.attr("type") == "password") {
                        input.attr("type", "text");
                    } else {
                        input.attr("type", "password");
                }
            });

            $(window).on('load', function() {
                $('#modalVerified').modal('show');
            });

            $('#addBtn').on('click', addForm)
            $('#removeBtn').on('click', removeForm)

            function addForm () {
                var new_chq_no = parseInt($('#totalForm').val()) + 1
                var new_input = "<br><input class='form-control' type='text' name='newRfid'>"

                $('#newForm').append(new_input)
                $('#totalForm').val(new_chq_no)
            }     
            
        })(jQuery);
